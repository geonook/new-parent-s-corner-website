{
  "name": "kcislk-parents-corner",
  "version": "1.0.0",
  "description": "KCISLK Parents' Corner - Public display website for parents of KCISLK Elementary School International Department",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 3002",
    "lint": "next lint",
    "start": "next start -p 8080",
    "db:generate": "prisma generate",
    "build:production": "NODE_ENV=production npm run db:generate && npm run build",
    "zeabur:build": "npm run db:generate && npm run build",
    "zeabur:start": "npm run start",
    "zeabur:deploy": "npm run db:generate && npm run start",
    "clean": "rm -rf .next out dist build",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-card": "1.1.0",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-tabs": "1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "4.1.0",
    "framer-motion": "latest",
    "lucide-react": "^0.454.0",
    "next": "^14.2.32",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.34.3",
    "tailwind-merge": "^2.5.5"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.4.5",
    "postcss": "^8.5",
    "prisma": "^5.21.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}